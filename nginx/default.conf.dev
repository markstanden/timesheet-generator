server {
  listen 80;
    location /socksjs-node {
    proxy_pass http://web-client;
    proxy_http_version 1.1;
    proxy_set_header Upgrade $http_upgrade;
    proxy_set_header Connection "Upgrade";


  location / {
    proxy_pass http://web-client:3000;
  }

  location /api {
    rewrite /api/(.*) /$1 break;
    proxy_pass http://api:5000;
  }

  location /native-web {
    rewrite /native-web/(.*) /$1 break;
    proxy_pass http://mobile-client:19006;
  }

  location /native-device {
    rewrite /native-device/(.*) /$1 break;
    proxy_pass http://mobile-client:19002;
  }
}